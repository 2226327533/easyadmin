<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE generatorConfiguration  
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"  
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

	<context id="DB2Tables" targetRuntime="MyBatis3" defaultModelType="flat">
		<plugin type="org.mybatis.generator.plugins.RenameExampleClassPlugin">
			<property name="searchString" value="[e|E]xample$" />
			<property name="replaceString" value="Criteria" />
		</plugin>

	<commentGenerator>
		<property name="suppressDate" value="true" />
		<!--  关闭自动生成的注释  -->
		 <property name="suppressAllComments" value="true" />
	</commentGenerator>

	<jdbcConnection driverClass="com.mysql.jdbc.Driver"
		connectionURL="jdbc:mysql://123.207.255.205/zyzl?useUnicode=true&amp;characterEncoding=UTF-8" userId="cms"
		password="Wo1993316.">
	</jdbcConnection>

	<javaTypeResolver>
		<property name="forceBigDecimals" value="false" />
	</javaTypeResolver>

	<!-- generate Model -->
	<javaModelGenerator targetPackage="wang.raye.zyzl.admin.model"
		targetProject="${entity.target.dir}">
		<property name="enableSubPackages" value="true" />
		<property name="trimStrings" value="true" />
	</javaModelGenerator>



	<!-- 自动生成xml（但是好像一直没有生成功，反正也不用xml）
	<sqlMapGenerator  targetPackage="wang.raye.zyzl.model.mapper"
		targetProject="${service.resources.dir}">
		<property name="enableSubPackages" value="true" />
	</sqlMapGenerator> -->

	<!-- 自动生成mapper接口, 可以是 ANNOTATEDMAPPER(注解), XMLMAPPER(xml), MIXEDMAPPER(不知道)-->
	<javaClientGenerator type="ANNOTATEDMAPPER"
		targetPackage="wang.raye.zyzl.admin.model.mapper" implementationPackage="wang.raye.zyzl.admin.model.mapper.impl"
		targetProject="${entity.target.dir}">
		<property name="enableSubPackages" value="true" />
	</javaClientGenerator>
	
	<table tableName="admin_user"  ></table>
		<table tableName="menu"  ></table>
		<table tableName="role"  ></table>


	</context>
</generatorConfiguration>  
